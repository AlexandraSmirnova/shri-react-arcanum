{
  "name": "shri-react-arcanum",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:server": "cross-env NODE_ENV=production  webpack --config webpack-server.config.js",
    "dev:server": "cross-env NODE_ENV=development webpack --config webpack-server.config.js --watch",
    "start:server": "node ./build/server/index.js",
    "start-dev:server": "nodemon ./build/server/index.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack-client.config.js",
    "dev:client": "cross-env NODE_ENV=development webpack --config webpack-client.config.js --watch",
    "build": "yarn clean && yarn build:client && yarn build:server && yarn start:server",
    "clean": "rimraf build",
    "test": "hermione"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "hermione": "^2.13.0",
    "hermione-custom-commands": "file:hermione-custom-commands",
    "mocha": "^6.2.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-react-loader": "^0.4.6",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "express": "^4.17.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
